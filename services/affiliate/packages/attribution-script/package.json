{
  "name": "@refref/attribution-script",
  "version": "0.1.0",
  "description": "Attribution script for tracking referral sources and managing attribution data",
  "type": "module",
  "main": "./dist/attribution-script.umd.js",
  "module": "./dist/attribution-script.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/attribution-script.es.js",
      "require": "./dist/attribution-script.umd.js",
      "types": "./dist/types/index.d.ts"
    },
    "./dist/*": "./dist/*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite --port 5174",
    "build": "infisical run --env=dev -- vite build --mode development || vite build --mode development",
    "build:prod": "infisical run --env=prod -- vite build --mode production || vite build --mode production",
    "clean": "rm -rf dist src/**/*.js src/**/*.js.map",
    "lint": "eslint . --report-unused-disable-directives",
    "test": "vitest run",
    "test:watch": "vitest",
    "preview": "vite preview",
    "type:check": "tsc --noEmit"
  },
  "keywords": [
    "attribution",
    "referral",
    "tracking",
    "analytics"
  ],
  "author": "",
  "devDependencies": {
    "@refref/eslint-config": "workspace:*",
    "@refref/typescript-config": "workspace:*",
    "jsdom": "^25.0.0",
    "typescript": "^5.9.2",
    "vite": "^6.3.3",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^3.1.4"
  }
}
